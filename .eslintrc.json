{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "indent": ["error", 2],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "object-curly-spacing": ["error", "always"],
    "array-bracket-spacing": ["error", "never"],
    "arrow-parens": ["error", "always"],
    "max-len": ["error", { "code": 100 }]
  }
}
